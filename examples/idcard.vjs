tag IdCard:
    prop name:string = ""
    prop age:number = 0

    method deleteData():
        self.setState({"name": "Anonyme", "age": 0});
    end

    method render():
        return <>
            <h1> "Carte d'identit√©"
            <ul>
                <li> {self.name}
                <li> {self.age}
            <button @click={self.deleteData}> "Delete all data"
        </>
    end
end

Versa.mount(
    <IdCard name="Thomas" age={17}>
)